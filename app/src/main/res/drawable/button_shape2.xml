<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!--设置按钮圆角按钮的步骤-->
    <!--1.设置圆角的半径-->
    <corners android:radius="60dp"/>
    <!--2.设置按钮的颜色-->
    <solid
        android:color="@color/lanbai"
        />
    <!--3.设置按钮边框粗细和颜色-->
    <stroke android:width="3dp"
        android:color="@color/white"/>
</shape>